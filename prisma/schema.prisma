generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        String   @id @default(cuid())
  nome      String
  contato   String
  vidas     Int
  idades    String    // Pode ser uma string com as idades separadas por v√≠rgula
  possuiCNPJ Boolean
  cep       String
  criadoEm  DateTime  @default(now())
}

